const mongoose=require('mongoose')

const studentSchema=mongoose.Schema({
    nom:{
        type:String,
        required:true
        
    },
    prenom:{
        type:String,
        required:true
    },
    email:{
        type:String,
        required:true
    },
    classe:{
       
        type:Number,
        required:true
        
    },
    

})
module.exports=mongoose.model("Student",studentSchema)